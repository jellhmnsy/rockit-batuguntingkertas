{
  "expo": {
    "name": "rockit",
    "slug": "rockit",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Splash Screen Group.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF8552"
    },
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FF8552"
      },
      "package": "com.nabil_mustofa.rockit"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android":{
            "usesCleartextTraffic": true
          },
          "ios": {
            "flipper": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2d76b9ab-a947-4df2-8605-bfba6fd7e71a"
      }
    }
  }
}
